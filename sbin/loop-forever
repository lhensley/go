#!/bin/bash
# loop-forever

# Include script setup file
source $(dirname $0)/script-setup

while true; do # Endless loop
	logger "$0: Sleeping for 10 seconds."
	sleep 10 # Sleep for 10 seconds, just in case
	logger "$0: Woke up."
	${LANE_CONFIG[script.master-loop.pathname]}
done # Endless loop

f_debug_variable "script.script-footer.pathname" "${LANE_CONFIG[script.script-footer.pathname]}"
source ${LANE_CONFIG[script.script-footer.pathname]}

exit 1