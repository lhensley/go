#!/bin/bash
# process_ts
# Usage: process_ts <input_file> <root_work_path> <root_storage_path>
# No final / on either path. For system root, use ""

INSTRING=$1
ROOT_WORK_PATH=$2
ROOT_STORAGE_PATH=$3

echo "In: $INSTRING"
echo "2 length: ${#2}"
echo "Prefix length: ${#ROOT_WORK_PATH}"
echo "Without prefix: ${$INSTRING: -${#ROOT_WORK_PATH}}"
echo ""
